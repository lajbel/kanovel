{
    "name": "kanovel",
    "version": "1.0.0-alpha.1",
    "description": "Make visual novels",
    "main": "./dist/kanovel.cjs",
    "module": "./dist/kanovel.mjs",
    "types": "./dist/kanovel.d.ts",
    "typesVersions": {
        "*": {
            "global": [
                "./dist/global.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": "./dist/kanovel.mjs",
            "require": "./dist/kanovel.cjs"
        },
        "./global": "./dist/kanovel.js"
    },
    "scripts": {
        "build": "node scripts/build.js",
        "game": "node test/build.js",
        "dev": "npm run build && npm run game"
    },
    "files": [
        "dist/",
        "src/",
        "CHANGELOG.md",
        "README.md"
    ],
    "keywords": [
        "game-engine",
        "kaboom",
        "plugin"
    ],
    "author": "lajbel <lajbelms@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "kaboom": "^3000.0.0-alpha.3"
    },
    "devDependencies": {
        "esbuild": "^0.15.5",
        "nodemon": "^2.0.20",
        "typescript": "^4.8.2"
    }
}
