{
    "name": "kanovel",
    "version": "1.0.0",
    "description": "Make visual novels in the browser",
    "main": "./dist/kanovel.cjs",
    "module": "./dist/kanovel.mjs",
    "types": "./dist/kanovel.d.ts",
    "typesVersions": {
        "*": {
            "global": [
                "./dist/global.d.ts"
            ]
        }
    },
    "exports": {
        "import": "./dist/kanovel.mjs",
        "require": "./dist/kanovel.cjs"
    },
    "scripts": {
        "build": "node scripts/build.js",
        "game": "cd test && node build.js"
    },
    "files": [
        "dist/",
        "src/",
        "CHANGELOG.md",
        "README.md"
    ],
    "keywords": [
        "game-engine",
        "kaboom",
        "plugin"
    ],
    "author": "lajbel <lajbelms@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "kaboom": "^2000.2.9"
    },
    "devDependencies": {
        "esbuild": "^0.15.5",
        "typescript": "^4.8.2"
    }
}
